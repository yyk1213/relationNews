		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<beans:import resource="classpath:applicationContext/applicationContext.xml" />
	<beans:import
		resource="classpath:applicationContext/applicationContext-batch.xml" />
	<beans:import
		resource="classpath:applicationContext/applicationContext-datasource.xml" />
</beans:beans>
